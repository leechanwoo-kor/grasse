<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="faq">

	<!-- 전체 FAQ 리스트 -->
	<select id="selectAllFaq" parameterType="hashmap" resultType="hashmap">
		<![CDATA[
			SELECT
				FAQ_NO,
				TITLE,
				CONTENT,
				REGDATE,
				MEMBER_ID
			FROM
				FAQ
		]]>
	</select>

</mapper>